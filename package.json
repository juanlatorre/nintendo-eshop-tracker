{
  "name": "nintendo-eshop-tracker-turborepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "prepare": "is-ci || husky install",
    "pretty": "pretty-quick --staged"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.32.6",
  "dependencies": {
    "pretty-quick": "^3.1.3"
  }
}
